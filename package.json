{
    "name": "@caffeine/api-docs",
    "version": "0.0.0",
    "type": "module",
    "author": {
        "name": "Alan Reis",
        "email": "alanreisanjo@gmail.com",
        "url": "https://hoyasumii.dev"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./*": {
            "types": "./dist/*/index.d.ts",
            "import": "./dist/*/index.js",
            "require": "./dist/*/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup 'src/**/index.ts' --format cjs,esm --dts --tsconfig tsconfig.build.json --clean",
        "test:unit": "vitest run",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage",
        "setup": "bun run build && bun link",
        "prepare": "husky"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.4.2",
        "@commitlint/config-conventional": "^20.4.2",
        "@faker-js/faker": "^10.3.0",
        "@types/bun": "latest",
        "@vitest/coverage-v8": "^4.0.18",
        "@vitest/ui": "^4.0.18",
        "husky": "^9.1.7",
        "openapi-types": "^12.1.3",
        "tsup": "^8.5.1",
        "vite-tsconfig-paths": "^6.1.1",
        "vitest": "^4.0.18",
        "elysia": "^1.4.26"
    },
    "peerDependencies": {
        "typescript": "^5.9.3",
        "@elysiajs/openapi": "^1.4.14",
        "elysia": "^1.4.26"
    }
}